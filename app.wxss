/*每个页面公共css */
.search {
  display: flex;
  padding: 10rpx 5rpx;
}
.search .search1 {
  width: 560rpx;
  height: 60rpx;
}
.search .search1 input {
  background: #f5f5f5;
  height: 60rpx;
  width: 600rpx;
  border: 1rpx solid #ccc;
  margin-left: 6rpx;
}
.search button {
  width: 120rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  padding: 0 6rpx;
  margin-left: 60rpx;
}
.side {
  overflow: hidden;
}
.side swiper {
  margin-top: 5rpx;
  height: 600rpx;
}
.side swiper image {
  height: 100%;
  width: 100%;
}
.side .list {
  display: flex;
  background: #b4b4b4;
  flex-wrap: wrap;
  text-align: center;
  justify-content: space-between;
}
.side .list .list1 {
  margin-top: 5rpx;
  background: #fff;
}
.side .list .list1 image {
  width: 186rpx;
  height: 100rpx;
}
